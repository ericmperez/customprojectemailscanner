# Gmail API Credentials
GMAIL_CLIENT_ID=your_gmail_client_id.apps.googleusercontent.com
GMAIL_CLIENT_SECRET=your_gmail_client_secret
GMAIL_REDIRECT_URI=http://localhost:3000/oauth2callback
GMAIL_REFRESH_TOKEN=your_refresh_token_will_be_added_after_setup

# Google Sheets Configuration
GOOGLE_SHEET_ID=your_google_sheet_id_from_url
SHEET_NAME=Licitaciones

# Supabase Configuration (for tracking processed emails)
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_KEY=your_supabase_anon_key

# Scheduler Configuration
# Cron format: */60 = every 60 minutes, */120 = every 120 minutes
SCHEDULE_INTERVAL_MINUTES=60

# Application Settings
NODE_ENV=production
LOG_LEVEL=info

